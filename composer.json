{
    "name": "burgeramt/burgeramt-termin",
    "description": "Get noticed when a termin is available",
    "keywords": [
        "termin"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "aws/aws-sdk-php": "^3.113",
        "guzzlehttp/guzzle": "^6.4",
        "laravel/lumen-framework": "^6.0",
        "laravel/tinker": "^1.0",
        "league/statsd": "^1.5"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Plista\\BurgeramtTermin\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
